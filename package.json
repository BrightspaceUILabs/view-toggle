{
  "name": "@brightspace-ui-labs/view-toggle",
  "version": "1.0.0",
  "description": "A lit-element component for toggling between views",
  "repository": "https://github.com/BrightspaceUILabs/view-toggle.git",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "view-toggle.js"
  ],
  "scripts": {
    "test:local": "polymer test --skip-plugin sauce",
    "test:sauce": "polymer test --skip-plugin local",
    "test:travis": "polymer test --config-file wct.conf-travis.json"
    "lint": "npm run lint:eslint && npm run lint:lit && npm run lint:style",
    "lint:eslint": "eslint . --ext .js,.html",
    "lint:lit": "lit-analyzer view-toggle.js --strict",
    "lint:style": "stylelint \"**/*.{js,html}\"",
    "start": "web-dev-server --app-index demo/index.html --node-resolve --open --watch",
    "test": "npm run lint && npm run test:headless",
  },
  "author": "D2L Corporation",
  "license": "Apache-2.0",
  "devDependencies": {
    "@polymer/test-fixture": "^4",
    "axe-core": "^3",
    "babel-eslint": "^10",
    "chai": "^4",
    "eslint": "^6",
    "eslint-config-brightspace": "^0.6",
    "eslint-plugin-html": "^6",
    "eslint-plugin-lit": "^1",
    "eslint-plugin-sort-class-members": "^1",
    "mocha": "^7.0.1",
    "polymer-cli": "^1",
    "puppeteer": "^2.1.0",
    "wct-mocha": "^1"
    "@babel/core": "^7",
    "@babel/eslint-parser": "^7",
    "@brightspace-ui/stylelint-config": "^0.7",
    "@web/dev-server": "^0.1",
    "eslint": "^8",
    "eslint-config-brightspace": "^0.20",
    "lit-analyzer": "^1",
    "stylelint": "^15"
  },
  "dependencies": {
    "@brightspace-ui/core": "^2",
    "lit": "^2"
  }
}
